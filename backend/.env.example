# MySQL Database Configuration
# MYSQL_HOST options:
#   'auto' - Automatically detect and use device's IP address (recommended)
#   'localhost' or '127.0.0.1' - Use local MySQL installation
#   Specific IP - Connect to remote MySQL server
MYSQL_HOST=auto
MYSQL_PORT=3306
MYSQL_USER=username
MYSQL_PASSWORD=password
MYSQL_DB=landwand_db

# Flask Configuration
SECRET_KEY=abcd1234
FLASK_ENV=development
PORT=5000
